# .env.example - Comprehensive Environment Configuration
# Kenya Health Agentic AI System - Kisumu, Busia, Vihiga Counties
# Copy this file to .env and configure with your actual values
# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
APP_NAME=kenya-health-ai
APP_VERSION=1.0.0
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO
SECRET_KEY=your_super_secret_key_change_in_production
# Application-specific settings
FOCUS_COUNTIES=Kisumu,Busia,Vihiga
DATA_START_DATE=2024-12-01
DATA_END_DATE=2025-06-30
DEFAULT_TIMEZONE=Africa/Nairobi
# =============================================================================
# DATABASE CONFIGURATION (Primary - Kenya Health Data)
# =============================================================================
DB_HOST=localhost
DB_PORT=5432
DB_NAME=kenya
DB_USER=postgres
DB_PASSWORD=your_database_password_here
DB_SSL_MODE=prefer
DB_MAX_CONNECTIONS=20
DB_CONNECTION_TIMEOUT=30
# Database backup configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *  # Daily at 2 AM
BACKUP_RETENTION_DAYS=30
BACKUP_STORAGE_PATH=/backups
# =============================================================================
# AI/LLM CONFIGURATION
# =============================================================================
# Primary LLM Provider - Hugging Face (Recommended)
HF_API_KEY=your_huggingface_api_key_here
HF_MODEL=mistralai/Mixtral-8x7B-Instruct-v0.1
HF_MAX_TOKENS=4000
HF_TEMPERATURE=0.1
HF_TIMEOUT=120
# Alternative LLM Providers (Configure as needed)
# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.1
# Groq Configuration (Fast Inference)
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=mixtral-8x7b-32768
# Anthropic Claude Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
# Local LLM Configuration (Optional)
LOCAL_LLM_ENABLED=false
LOCAL_LLM_URL=http://localhost:1234/v1
LOCAL_LLM_MODEL=local-mixtral
# =============================================================================
# AUTOGEN CONFIGURATION
# =============================================================================
AUTOGEN_VERSION=0.6.0
AUTOGEN_MAX_ROUNDS=12
AUTOGEN_CACHE_SEED=42
AUTOGEN_TIMEOUT=300
AUTOGEN_SPEAKER_SELECTION=round_robin
AUTOGEN_ENABLE_LOGGING=true
# Agent-specific settings
SQL_AGENT_ENABLED=true
ANALYSIS_AGENT_ENABLED=true
VIZ_AGENT_ENABLED=true
MEMORY_AGENT_ENABLED=true
# =============================================================================
# EMBEDDINGS AND VECTOR SEARCH
# =============================================================================
EMBEDDINGS_MODEL=all-MiniLM-L6-v2
EMBEDDINGS_DIMENSION=384
EMBEDDINGS_BATCH_SIZE=32
EMBEDDINGS_CACHE_SIZE=1000
# FAISS Configuration
FAISS_INDEX_TYPE=IndexFlatIP
FAISS_SAVE_INTERVAL=300  # seconds
FAISS_INDEX_PATH=embeddings/faiss_index.bin
# Alternative Vector Databases (Optional)
CHROMA_ENABLED=false
CHROMA_HOST=localhost
CHROMA_PORT=8000
CHROMA_COLLECTION=kenya_health_queries
PINECONE_ENABLED=false
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-west1-gcp-free
PINECONE_INDEX=kenya-health-index
# =============================================================================
# MEMORY AND CACHING CONFIGURATION
# =============================================================================
MEMORY_ENABLED=true
MEMORY_MAX_ENTRIES=1000
MEMORY_FILE_PATH=memory/chat_memory.json
MEMORY_CLEANUP_INTERVAL=86400  # 24 hours in seconds
# Cache Configuration
CACHE_ENABLED=true
CACHE_TYPE=sqlite  # Options: sqlite, redis, memory
CACHE_EXPIRY_DAYS=7
CACHE_MAX_SIZE=500MB
CACHE_DB_PATH=memory/query_cache.db
# Redis Configuration (if using Redis caching)
REDIS_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password_here
REDIS_SOCKET_TIMEOUT=30
REDIS_CONNECTION_POOL_SIZE=10
# =============================================================================
# STREAMLIT CONFIGURATION
# =============================================================================
STREAMLIT_PORT=8501
STREAMLIT_HOST=0.0.0.0
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_SERVER_ENABLE_CORS=false
STREAMLIT_SERVER_ENABLE_XSRF_PROTECTION=true
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false
STREAMLIT_THEME_BASE=light
STREAMLIT_THEME_PRIMARY_COLOR=#1976d2
# =============================================================================
# SECURITY AND AUTHENTICATION
# =============================================================================
# API Security
API_RATE_LIMIT=100  # requests per minute
API_RATE_LIMIT_WINDOW=60  # seconds
ALLOWED_HOSTS=localhost,127.0.0.1,example.com
CORS_ORIGINS=http://localhost:8501,https://example.com
# Session Management
SESSION_TIMEOUT=3600  # 1 hour in seconds
SESSION_COOKIE_SECURE=false  # Set to true in production with HTTPS
SESSION_COOKIE_HTTPONLY=true
SESSION_COOKIE_SAMESITE=Strict
# Data Encryption
ENCRYPTION_KEY=your_encryption_key_here
DATA_ENCRYPTION_ENABLED=false  # Enable in production
# =============================================================================
# REPORT GENERATION CONFIGURATION
# =============================================================================
REPORTS_ENABLED=true
PDF_GENERATION_ENABLED=true
EXCEL_EXPORT_ENABLED=true
MAX_REPORT_ROWS=10000
REPORT_CACHE_ENABLED=true
REPORT_CACHE_TTL=3600  # 1 hour
# PDF Generation Settings
PDF_ENGINE=xhtml2pdf  # Options: xhtml2pdf, weasyprint
PDF_QUALITY=high
PDF_COMPRESSION=true
# Report Storage
REPORT_STORAGE_PATH=reports/
REPORT_RETENTION_DAYS=90
REPORT_AUTO_CLEANUP=true
# =============================================================================
# MONITORING AND LOGGING
# =============================================================================
MONITORING_ENABLED=true
METRICS_COLLECTION_INTERVAL=60  # seconds
HEALTH_CHECK_INTERVAL=30  # seconds
PERFORMANCE_MONITORING=true
# Logging Configuration
LOG_FORMAT=json
LOG_FILE_PATH=logs/app.log
LOG_MAX_SIZE=100MB
LOG_BACKUP_COUNT=5
LOG_ROTATION=daily
# Error Tracking (Sentry)
SENTRY_ENABLED=false
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development
SENTRY_TRACES_SAMPLE_RATE=0.1
# Metrics and Analytics
PROMETHEUS_ENABLED=false
PROMETHEUS_PORT=8000
GRAFANA_ENABLED=false
# =============================================================================
# CLOUD STORAGE CONFIGURATION
# =============================================================================
# Google Cloud Platform
GCP_ENABLED=false
GCP_PROJECT_ID=your_gcp_project_id
GCP_REGION=us-central1
GCP_BUCKET_NAME=your_gcp_bucket_name
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json
GCP_CLOUD_SQL_INSTANCE=your_gcp_cloud_sql_instance
# Amazon Web Services
AWS_ENABLED=false
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_aws_s3_bucket
AWS_RDS_ENDPOINT=your_rds_endpoint.amazonaws.com
# Microsoft Azure
AZURE_ENABLED=false
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_RESOURCE_GROUP=your_azure_resource_group
AZURE_STORAGE_ACCOUNT=your_azure_storage_account
AZURE_STORAGE_KEY=your_azure_storage_key
AZURE_CONTAINER_NAME=health-data
# =============================================================================
# NOTIFICATION CONFIGURATION
# =============================================================================
# Slack Integration
SLACK_ENABLED=false
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
SLACK_CHANNEL=#health-alerts
SLACK_USERNAME=Your_App_Name
SLACK_ICON_EMOJI=:hospital:
# Microsoft Teams
TEAMS_ENABLED=false
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your/teams/webhook
# Email Notifications
EMAIL_ENABLED=false
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your_email@example.com
EMAIL_PASSWORD=your_email_password_here
EMAIL_USE_TLS=true
EMAIL_FROM_ADDRESS=noreply@example.com
# WhatsApp Business API (Optional)
WHATSAPP_ENABLED=false
WHATSAPP_API_KEY=your_whatsapp_api_key
WHATSAPP_PHONE_NUMBER=+254700000000
# =============================================================================
# PERFORMANCE OPTIMIZATION
# =============================================================================
PERFORMANCE_MONITORING=true
QUERY_TIMEOUT=120  # seconds
MAX_CONCURRENT_QUERIES=5
MEMORY_LIMIT=4GB
CPU_LIMIT=2
# Data Processing Optimization
PANDAS_COPY_ON_WRITE=true
NUMPY_MULTITHREADING=true
POLARS_ENABLED=false  # Use Polars for large datasets
# Chart Generation Optimization
PLOTLY_RENDERER=browser
CHART_CACHE_ENABLED=true
CHART_CACHE_TTL=1800  # 30 minutes
# =============================================================================
# DEVELOPMENT AND TESTING
# =============================================================================
TESTING_MODE=false
MOCK_DATA_ENABLED=false
SEED_TEST_DATA=false
DEV_TOOLS_ENABLED=true
HOT_RELOAD=true
# Test Database (for development)
TEST_DB_HOST=localhost
TEST_DB_PORT=5433
TEST_DB_NAME=kenya_test
TEST_DB_USER=postgres
TEST_DB_PASSWORD=test_password_here
# =============================================================================
# FEATURE FLAGS
# =============================================================================
FEATURE_EMBEDDINGS=true
FEATURE_PDF_REPORTS=true
FEATURE_EXCEL_EXPORT=true
FEATURE_ADVANCED_CHARTS=true
FEATURE_REAL_TIME_UPDATES=false
FEATURE_VOICE_INTERFACE=false
FEATURE_MOBILE_RESPONSIVE=true
FEATURE_DARK_MODE=false
FEATURE_MULTI_LANGUAGE=false
# County-specific Features
FEATURE_KISUMU_ANALYSIS=true
FEATURE_BUSIA_ANALYSIS=true
FEATURE_VIHIGA_ANALYSIS=true
FEATURE_CROSS_COUNTY_COMPARISON=true
# Advanced AI Features
FEATURE_PREDICTIVE_ANALYTICS=false
FEATURE_ANOMALY_DETECTION=false
FEATURE_RECOMMENDATION_ENGINE=true
FEATURE_NATURAL_LANGUAGE_GENERATION=true
# =============================================================================
# DATA QUALITY AND VALIDATION
# =============================================================================
DATA_VALIDATION_ENABLED=true
DATA_QUALITY_THRESHOLD=0.95  # 95% data completeness required
OUTLIER_DETECTION_ENABLED=true
MISSING_DATA_IMPUTATION=false
# Data Freshness Monitoring
DATA_FRESHNESS_CHECK=true
MAX_DATA_AGE_DAYS=7  # Alert if data is older than 7 days
DATA_UPDATE_FREQUENCY=daily
# =============================================================================
# BACKUP AND DISASTER RECOVERY
# =============================================================================
BACKUP_STRATEGY=daily
BACKUP_ENCRYPTION=true
DISASTER_RECOVERY_ENABLED=true
RTO_TARGET=4  # Recovery Time Objective in hours
RPO_TARGET=1  # Recovery Point Objective in hours
# Backup Destinations
BACKUP_LOCAL_PATH=/backups/local
BACKUP_CLOUD_ENABLED=false
BACKUP_CLOUD_PROVIDER=gcp  # Options: gcp, aws, azure
# =============================================================================
# COMPLIANCE AND GOVERNANCE
# =============================================================================
GDPR_COMPLIANCE=false  # Set to true if handling EU data
HIPAA_COMPLIANCE=false  # Set to true if handling PHI
DATA_RETENTION_DAYS=2555  # 7 years for health data
AUDIT_LOGGING=true
ACCESS_LOGGING=true
# Data Anonymization
ANONYMIZATION_ENABLED=false
PII_DETECTION_ENABLED=false
DATA_MASKING_ENABLED=false
# =============================================================================
# CUSTOM HEALTH SYSTEM CONFIGURATION
# =============================================================================
# Kenya-specific Configuration
KENYA_TIME_ZONE=Africa/Nairobi
HEALTH_SYSTEM_VERSION=2024.1
MFH_INTEGRATION=false  # Ministry of Health integration
DHIS2_INTEGRATION=false  # DHIS2 integration
# CHW Performance Benchmarks
EXCELLENT_PERFORMANCE_THRESHOLD=85
GOOD_PERFORMANCE_THRESHOLD=70
CRITICAL_PERFORMANCE_THRESHOLD=50
INTERVENTION_SCORE_THRESHOLD=40
# Service Area Weights (for composite scoring)
PNEUMONIA_WEIGHT=0.15
MALARIA_WEIGHT=0.15
FAMILY_PLANNING_WEIGHT=0.20
IMMUNIZATION_WEIGHT=0.15
NUTRITION_WEIGHT=0.15
NEWBORN_CARE_WEIGHT=0.20
# Resource Availability Targets
TOOLS_AVAILABILITY_TARGET=90
PPE_AVAILABILITY_TARGET=95
MEDICINES_AVAILABILITY_TARGET=85
# =============================================================================
# DEPLOYMENT ENVIRONMENT OVERRIDES
# =============================================================================
# Production Environment (uncomment and modify for production)
# APP_ENV=production
# DEBUG=false
# LOG_LEVEL=WARNING
# STREAMLIT_SERVER_HEADLESS=true
# SESSION_COOKIE_SECURE=true
# DATA_ENCRYPTION_ENABLED=true
# BACKUP_ENCRYPTION=true
# SENTRY_ENABLED=true
# MONITORING_ENABLED=true
# Staging Environment (uncomment for staging)
# APP_ENV=staging
# DEBUG=true
# LOG_LEVEL=INFO
# SENTRY_ENABLED=true
# Local Development (default settings above)
# APP_ENV=development
# DEBUG=true
# LOG_LEVEL=DEBUG
# HOT_RELOAD=true
# =============================================================================
# NOTES AND INSTRUCTIONS
# =============================================================================
# 1. SECURITY: Change all default passwords and keys before deployment
# 2. API KEYS: Obtain API keys from respective providers (HuggingFace, OpenAI, etc.)
# 3. DATABASE: Ensure PostgreSQL is running and accessible
# 4. STORAGE: Create necessary directories for logs, backups, reports
# 5. MONITORING: Configure monitoring tools in production
# 6. BACKUP: Set up automated backup procedures
# 7. SSL/TLS: Enable HTTPS in production environments
# 8. FIREWALL: Configure appropriate firewall rules
# 9. SCALING: Adjust resource limits based on expected load
# 10. COMPLIANCE: Review data handling requirements for your jurisdiction
# For detailed setup instructions, see README.md
# For troubleshooting, see docs/troubleshooting.md
# For security guidelines, see docs/security.md